                android:layout_marginTop="@dimen/_1pxv"
                android:layout_marginBottom="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintGroup86">

                <TextView
                    android:id="@+id/txtCharH"
                    style="@style/txtIntermedium11"
                    android:layout_width="44dp"
                    android:layout_height="15dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_char_h"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.045"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="@string/lbl_char_h" />

                <TextView
                    android:id="@+id/txt12SecondsAgo"
                    style="@style/txtIntermedium10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_197pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_12_seconds_ago"
                    app:layout_constraintBaseline_toBaselineOf="@+id/txtCharH"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/txtCharH"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="@string/lbl_12_seconds_ago" />
