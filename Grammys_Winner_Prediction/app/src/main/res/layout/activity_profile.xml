                            android:src="@drawable/img_profile"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/txtCharHuang"
                            app:layout_constraintHorizontal_bias="0.11"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/txtCharHuang"
                            style="@style/txtInterbold16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_marginBottom="@dimen/_21pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_char_huang"
                            app:layout_constraintBottom_toBottomOf="@+id/imageProfile"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.09"
                            app:layout_constraintStart_toEndOf="@+id/imageProfile"
                            app:layout_constraintTop_toTopOf="@+id/imageProfile"
                            tools:text="@string/lbl_char_huang" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.appcompat.widget.AppCompatButton
