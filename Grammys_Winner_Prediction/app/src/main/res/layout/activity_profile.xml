            android:layout_height="40dp"
            android:background="@color/gray_800"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.118"
            app:tabIconTint="@color/amber_600"
            app:tabIndicatorColor="@color/amber_600"
            app:tabIndicatorHeight="5dp"
            app:tabRippleColor="@color/black_900_40"
            app:tabTextAppearance="@style/txtIntermedium14"
            app:tabTextColor="@color/gray_300">

            <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lbl_posts_4" />

            <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lbl_friends_30" />

        </com.google.android.material.tabs.TabLayout>
