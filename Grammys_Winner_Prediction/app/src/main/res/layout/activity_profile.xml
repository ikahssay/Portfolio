                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_best_new_artist"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:text="@string/lbl_best_new_artist" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/txtOliviaRodrigo"
                    style="@style/txtIntermedium14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_70pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_olivia_rodrigo"
