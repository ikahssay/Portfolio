                        android:src="@drawable/img_upvote20"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/imageDownvote4"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/imageComment"
                        android:layout_width="@dimen/_21pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_comment"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.3"
                        app:layout_constraintStart_toEndOf="@+id/imageDownvote4"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintGroup79"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_70pxh"
                    android:layout_marginEnd="@dimen/_3pxh"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageForwardButton"
                    app:layout_constraintHorizontal_bias="0.958"
                    app:layout_constraintStart_toEndOf="@+id/txtOliviaRodrigo"
                    app:layout_constraintTop_toBottomOf="@+id/constraintGroup78">

                    <TextView
                        android:id="@+id/txt20"
                        style="@style/txtIntersemibold11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_20"
                        app:layout_constraintEnd_toStartOf="@+id/txt4"
                        app:layout_constraintHorizontal_bias="0.04"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:text="@string/lbl_20" />

                    <TextView
                        android:id="@+id/txt4"
                        style="@style/txtIntersemibold11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_4"
                        app:layout_constraintEnd_toStartOf="@+id/txt5"
