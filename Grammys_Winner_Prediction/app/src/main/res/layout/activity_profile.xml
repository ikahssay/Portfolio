                    app:layout_constraintEnd_toStartOf="@+id/imageForwardButton"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageDownvote4"
                        android:layout_width="@dimen/_21pxh"
                        android:layout_height="@dimen/_20pxh"
